{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "multi_board" "truck" "vision_lev02" "tractor" "tractor_heavy"}
	{able "emit_on_attack"}
	{extension "gaz_aaa.mdl"}
	{collider "truck"}
	{patherId "gazaaa"}

	{extender "enumerator"
		{digit_folder "ru1"}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))

	("shield_cover")

	{volume "body7" {able {holed}}}
	{volume "bodyback1" {material "wood"} {tags "wood"} {able {holed}}}
	{volume "bodyback2" {material "wood"} {tags "wood"}}

	{volume "shield_cover" {able {holed}}}

	("crew_2_human")
	("passenger_8_human")

	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)

	{boarder
		{anm "passenger"
			{forward {begin "board_body"} {end "seat_passenger"} {base "board"}}
			{reverse {end "emit_body" 1} {base "board"}}
		}
	}

	{extender "shield" {on}}

	{extender "inventory"
		{box
		}
	}

	{mass 2475}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(65) ;----------------------------------Max speed.
			speedatmaxturn(17) ;-------------------------Max speed at max turn.
			reverse(20) ;--------------------------------Max speed in Reverse.
			weight(2.475) ;------------------------------Weight in Tonnes.
			power(50) ;----------------------------------Horse Power.
			fuel(100) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(145) ;---------------------------------Range in Kilometers.
		)
	}
}