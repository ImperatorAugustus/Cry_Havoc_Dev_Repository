{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "turret" "with_gun" "open_top" "automatic" "truck" "vision_lev02" "muzzle_20mm_25mm_auto" "tractor" "tractor_heavy"}
	{able "emit_on_attack"}
	{extension "gaz_aaa_72k.mdl"}
	{extension "crew.ext"}
	{collider "truck"}
	{patherId "gazaaa"}
	{targetSelector "tank_zenite"}

	{extender "enumerator"
		{digit_folder "ru1"}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	("shield_group" args "shield1" 4.5 "shield1")
	("shield_group" args "shield2" 4.5 "shield1")
	("shield" args "shield3" 4.5)

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))
	("gun" repair(30) thickness(15))
	("turret" repair(45))

	{volume "body7" {able {holed}}}
	{volume "shield1" {able {holed}}}
	{volume "shield2" {able {holed}}}
	{volume "bodyback1" {material "wood"} {tags "wood"}}
	{volume "bodyback2" {material "wood"} {tags "wood"}}
	{volume "bodyback3" {material "wood"} {tags "wood"}}
	{volume "bodyback4" {material "wood"} {tags "wood"}}
	{volume "bodyback5" {material "wood"} {tags "wood"}}

	{volume "fuel" {able {NoWoundDecal 0}}}
	{volume "transmission" {able {NoWoundDecal 0}}}
	{volume "turret1" {thickness 8}}
	{volume "turret2" {thickness 8}}
	{volume "turret3" {thickness 8}}
	{volume "turret4" {thickness 8}}

	{extender "inventory"
		{box
			{item "bulletrus_25_auto apbc" 210}
			{item "bulletrus_25_auto he" 210}
		}
	}

	{Weaponry
		("restore_ik_time")
		{place "gun"
			{weapon "25mm_72k" filling "bulletrus_25_auto he" 7}
			("abm_dymamic_aaa"
				zeroing(1.2)
				dispersion(0.15)
			)
		}
	}

	{mass 2475}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(65) ;----------------------------------Max speed.
			speedatmaxturn(17) ;-------------------------Max speed at max turn.
			reverse(20) ;--------------------------------Max speed in Reverse.
			weight(2.475) ;------------------------------Weight in Tonnes.
			power(50) ;----------------------------------Horse Power.
			fuel(100) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(145) ;---------------------------------Range in Kilometers.
		)
	}
	{bone "gun_rot"
		{limits -4 88}
		{speed2 60}
	}
	{bone "turret"
		{speed2 60}
	}
}