{game_entity
	(include "/properties/armoredcar.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.0))
	{props "tank" "turret" "wheeled" "with_gun" "vision_lev02" "muzzle_20mm_25mm_auto"}
	{extension "sdkfz231.mdl"}
	{extension "crew.ext"}
	(include "sdkfz231.ext")
	{targetclass "armoredcar"}
	{targetSelector "tank_light"}
	{patherId "sdkfz232"}

	{extender "enumerator"
		{digit_folder "np"}
	}

	{sensor
		{visor "main"
			{vision "recon_main"}
		}
	}

	{extender "shield" {on}}

	{extender "inventory"
		{box
			{item "bulletger_20_auto ap" 70}	; 18 magazines (10 rounds each)
			{item "bulletger_20_auto apcr" 30}
			{item "bulletger_20_auto he" 70}
			{item "ammo mgun_ger belt" 1950}	; 14 belts (150 rounds each)
			{item "geballteladung_3kg" 1}
		}
	}

	{Weaponry
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "20mm_kwk30" filling "bulletger_20_auto ap" 10}
			{gunner "gunner"}
			{charger "charger"}
			("abm_dymamic_autocannon"

			)
		}
		{place "mgun2"
			{type "coaxial"}
			{weapon "mg34_tank" filled}
			{foresight "foresight5"}
			{gunner "gunner"}
			{charger "charger"}
			("abm_mgun")
			{basic}
		}
	}

	{mass 8300}
	{Chassis "wheel"
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_btr"
			speed(85) ;----------------------------------Max speed.
			speedatmaxturn(25) ;-------------------------Max speed at max turn.
			reverse(85) ;--------------------------------Max speed in Reverse.
			weight(8.3) ;--------------------------------Weight in Tonnes.
			power(180) ;---------------------------------Horse Power.
			fuel(180) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(170) ;---------------------------------Range in Kilometers.
		)
	}

	{bone "gun_rot"
		{limits -10 20}
		{speed2 7.5}
	}
	{bone "turret"
		{speed2 7.5}
	}
}