{game_entity
	(include "/properties/armoredcar.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.0))
	{props "tank" "halftrack" "wheeled" "multi_board" "explosion_throw_crew" "vision_lev02" "tractor" "tractor_heavy" "detect_tank_mg" "open_death_fx_low" "muzzle_37mm_47mm_sides" "turret" "with_gun" "shell_eject" "open_top"}
	{extension "sdkfz250a_11.mdl"}
	(include "sdkfz250a_11.ext")
	;{extension "crew.ext"}
	{patherId "sdkfz251"}
	{targetclass "armoredcar"}
	{targetSelector "cannon_small"}

	{extender "enumerator"
		{digit_folder "ger"}
	}

	{boarder
		{anm "passenger1"
			{forward {begin "board_sdkfz250a"} {end "pose_sdkfz250a_passenger1"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "passenger3"
			{forward {begin "board_sdkfz250a"} {end "halftrack_pass_l_02_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "driver"
			{forward {begin "board_sdkfz250a"} {end "pose_sdkfz250a_driver"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "gunner"
			{forward {begin "board_sdkfz250a"} {end "pose_sdkfz250a11_gunner"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "gunner2"
			{forward {begin "board_sdkfz250a"} {end "halftrack_gunner_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "charger"
			{forward {begin "board_sdkfz250a"} {end "pose_sdkfz250a11_charger"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
	}

	("crew" place(driver)		bone(driver)	door(emit1)		animation(driver)		visor(driver)	off({sensor}))
	("crew" place(gunner)		bone(gunner)	door(emit1)		animation(gunner)		visor(main)		off({sensor}))
	("crew" place(gunner4)		bone(gunner4)	door(emit1)		animation(gunner2)		visor(none)		off())
	("crew" place(charger)		bone(charger)	door(emit1)		animation(charger)		visor(none)		off())

	("passenger" place(seat1) bone(seat01) door(emit1)  animation(passenger1)  off())
	("passenger" place(seat3) bone(seat03) door(emit1)  animation(passenger3)  off())

	{sensor
		{visor "human"
			{vision "none"}
		}
		{visor "around"
			{vision "none"}
		}
	}

	{extender "inventory"
		{weapon "mgun2" {mask "mountable"}}
		{box
			{item "bulletger_28 apcr" 99}		; 168 rounds
			{item "bulletger_28 he" 68}
			{item "mg34_drum" filled {user "mgun2"}}
			{item "ammo mgun_ger belt" 1100}
		}
	}

	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "28mm_pzb41" filling "bulletger_28 apcr" 1}
			{gunner "gunner"}
			{charger "charger"}
			("abm_dymamic_gun"

			)
		}
		{place "mgun2"
			{type "rear"}
			{gunner "gunner4"}
			{charger "gunner4"}
			{LinkBone "mgun2"}
			{LinkAnimation "off"}
			("abm_mgun")
			{ai_in_manual}
			{basic}
		}
	}

	{mass 5530}
	{Chassis "wheel"
		("tank_trace_ex" fx("tracks_sm2") step(0.2) len(0.29) boneleft("FXtraceL3") boneright("FXtraceR3"))
		("car_trace" fx("tracewheel03a") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))

		("mobility_car"
			speed(60) ;----------------------------------Max speed.
			speedatmaxturn(18) ;-------------------------Max speed at max turn.
			reverse(18) ;--------------------------------Max speed in Reverse.
			weight(5.53) ;-------------------------------Weight in Tonnes.
			power(100) ;---------------------------------Horse Power.
			fuel(140) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(200) ;---------------------------------Range in Kilometers.
		)
	}
	{bone "gun_rot"
		{limits -10 25}
		{speed2 10}
	}
	{bone "turret"
		{limits -20 20}
		{speed2 12.5}
	}
	{bone "mgun_a2_rot1"
		{speed2 30}
	}
	{bone "mgun_a2_rot2"
		{speed2 30}
	}
}