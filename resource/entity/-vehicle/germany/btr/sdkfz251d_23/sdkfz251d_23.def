{game_entity
	(include "/properties/armoredcar.ext")
	(include "/properties/selection/vehicle.inc" scale(1.0))
	{props "tank" "halftrack" "wheeled" "multi_board" "explosion_throw_crew" "vision_lev02" "tractor" "tractor_heavy" "detect_tank_mg" "open_death_fx_low"}
	{able "emit_passengers_on_enemy"}
	{extension "sdkfz251d_23.mdl"}
	(include "sdkfz251d_23.ext")
	{patherId "sdkfz251"}
	{targetclass "armoredcar"}
	{targetSelector "tank_bullet"}
	
	{extender "enumerator"
		{digit_folder "np"}
	}

	{boarder
		{anm "passenger5"
			{forward {begin "halftrack_pass_l_03_board"} {end "halftrack_pass_l_03_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "passenger6"
			{forward {begin "halftrack_pass_r_03_board"} {end "halftrack_pass_r_03_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "passenger7"
			{forward {begin "halftrack_pass_l_04_board"} {end "halftrack_pass_l_04_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "passenger8"
			{forward {begin "halftrack_pass_r_04_board"} {end "halftrack_pass_r_04_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "driver"
			{forward {begin "halftrack_driver_board"} {end "halftrack_driver_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "charger"
			{forward {begin "halftrack_commander_board"} {end "halftrack_commander_board_seat"} {base "board"}}
			{reverse {end "emit_sd222_driver"} {base "board"}}
		}
		{anm "gunner"
			{forward {begin "board_sd222_driver"} {end "seat_driver_car"}}
			{reverse {end "emit_sd222_driver"}}
		}
		{anm "commander"
			{forward {begin "board_sd222_driver"} {end "seat_driver_car"}}
			{reverse {end "emit_sd222_driver"}}
		}
	}

	("crew" place(driver)		bone(driver)	door(emit1)		animation(driver)		visor(driver)	off({sensor}))
	("crew" place(gunner)		bone(gunner)	door(emit2)		animation(gunner)		visor(none)		off())
	("crew" place(charger)		bone(charger)	door(emit1)		animation(charger)		visor(driver)	off({sensor}))
	("crew" place(commander) 	bone(commander) 	door(emit2)  		animation(commander)		visor(none)		off())

	("passenger" place(seat5) bone(seat5) door(emit1)  animation(passenger5)  off())
	("passenger" place(seat6) bone(seat6) door(emit2)  animation(passenger6)  off())
	("passenger" place(seat7) bone(seat7) door(emit1)  animation(passenger7)  off())
	("passenger" place(seat8) bone(seat8) door(emit2)  animation(passenger8)  off())

	{sensor
		{visor "main"
			{vision "recon_main"}
		}
		{visor "around"
			{vision "vehicle_around"}		;// better vision than a in-closed turret 
		}
	}
;	{extender "shield" {on}}
	{extender "inventory"
		{weapon "mgun" {mask "mountable"}}
		{box
			{item "bulletger_20_auto ap" 100}		; 25 magazines (10 rounds each)
			{item "bulletger_20_auto apcr" 10}
			{item "bulletger_20_auto he" 130}
			{item "ammo mgun_ger belt ap" 2250}		; 16 belts (150 rounds each)
			{item "geballteladung_3kg" 1}
			{item "mg42_tank" filling "ammo mgun_ger belt ap" 150 {user "mgun"}}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "20mm_kwk38" filling "bulletger_20_auto ap" 10}
			{gunner "gunner"}
			{charger "commander"}
			("abm_dymamic_autocannon"
				zeroing(2)
				dispersion(0.15)
			)
		}
		{place "mgun"
			{type "coaxial"}
			{gunner "gunner"}
			{charger "commander"}
			{LinkBone "mgun"}
			{LinkAnimation "off"}
			("abm_mgun")
			{basic}
		}
	}
	{mass 7810}
	{Chassis "wheel"
		("tank_trace_ex" fx("tracks_sm2") step(0.2) len(0.49) boneleft("FXtraceL3") boneright("FXtraceR3"))
		("car_trace" fx("tracewheel03a") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("mobility_car"
			speed(53) ;----------------------------------Max speed.
			speedatmaxturn(14) ;-------------------------Max speed at max turn.
			reverse(9) ;---------------------------------Max speed in Reverse.
			weight(7.81) ;--------------------------------Weight in Tonnes.
			power(100) ;---------------------------------Horse Power.
			fuel(160) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(150) ;---------------------------------Range in Kilometers.
		)
	}

	{bone "gun_rot"
		{limits -4 70}		;// AA mode limits
		;{limits -4 4}		;// hatch closed limits
		{speed2 20}
	}
	{bone "turret"
		{speed2 25}
	}
}