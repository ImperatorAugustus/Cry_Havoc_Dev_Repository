{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "turret" "with_gun" "open_top" "automatic" "multi_board" "truck" "vision_lev02" "muzzle_20mm_25mm_auto" "tractor" "tractor_heavy" "detect_tank_light"}
	{able "emit_on_attack"}
	{extension "blitz3_6_flak30.mdl"}
	{collider "truck"}
	{patherId "blitz36"}
	{extension "crew.ext"}
	{targetSelector "tank_zenite"}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("glass" args "glassr")
	("glass" args "glassl")
	("glass" args "glass")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))
	("gun" repair(30) thickness(15))
	("turret" repair(45))

	{volume "body6" {able {holed}}}
	{volume "fuel" {able {NoWoundDecal 0}}}
	{volume "transmission" {able {NoWoundDecal 0}}}
	{volume "turret" {thickness 8} {able {holed}}}
	{volume "turret2" {thickness 8}}

	("wood" args "bodyback1")
	("wood" args "bodyback2")
	("wood" args "bodyback3")
	("wood" args "bodyback4")
	("wood" args "bodyback5")

	{extender "inventory"
		{box
			{item "bulletger_20_auto ap" 240}
			{item "bulletger_20_auto he" 400}
		}
	}

	{Weaponry
		("restore_ik_time")
		{place "gun"
			{weapon "20mm_flak30" filling "bulletger_20_auto he" 20}
			("abm_dymamic_aaa"
				zeroing(2.9)
				dispersion(0.08)
			)
		}
	}

	{mass 3200}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(60) ;----------------------------------Max speed.
			speedatmaxturn(15) ;-------------------------Max speed at max turn.
			reverse(32) ;--------------------------------Max speed in Reverse.
			weight(3.2) ;--------------------------------Weight in Tonnes.
			power(68) ;----------------------------------Horse Power.
			fuel(80) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(230) ;---------------------------------Range in Kilometers.
		)
	}
	{bone "gun_rot"
		{limits -5 88}
		{speed2 60}
	}
	{bone "turret"
		{limits -145 145}
		{speed2 60}
	}
}
