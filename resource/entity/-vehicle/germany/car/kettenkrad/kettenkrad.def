{game_entity
	(include "/properties/halftrack.ext")
	(include "/properties/shadow/tank.inc" scale(0.7))
	(include "/properties/selection/vehicle.inc" scale(0.55))
	{props "vision_lev01" "tractor" "tractor_light" "explosion_throw_crew"}
	{able "emit_passengers_on_enemy"}
	{extension "kettenkrad.mdl"}
	{patherId "ketten"}

	("wheel" args "wheelfront")
	("tracks" repair(30) thickness(15))

	("fuel" args "fuel1")
	("fuel" args "fuel2")
	("transmission" args "transmission")

	("car_thickness" body(3) engine(2))

	{volume "body1" {able {holed}}}
	{volume "body2" {able {holed}}}

	("crew_human_2_v" place(driver) bone(driver) door(emit1) door2(emit2))
	("crew_human_v" place(seat1) bone(seat01) door(emit3) animation(passenger1))
	("crew_human_v" place(seat2) bone(seat02) door(emit4) animation(passenger2))

	{placer
		{place "driver"
			{visor "main"}
			{UseVision 0}
		}
	}

	{sensor
		{visor "main"
			{vision "none"}
		}
		{visor "around"
			{vision "none"}
		}
	}

	{boarder
		{anm "left"
			{forward {begin "board_sdkfz10_driver"} {end "seat_kettenkrad_driver"}}
			{reverse {end "" -1}}
		}
		{anm "right"
			{forward {begin ""} {end "seat_kettenkrad_driver"}}
			{reverse {end "" -1}}
		}
		{anm "passenger1"
			{forward {begin ""} {end "seat_kubel_passenger1"}}
			{reverse {end "" -1}}
		}
		{anm "passenger2"
			{forward {begin ""} {end "seat_kubel_passenger2"}}
			{reverse {end "" -1}}
		}
	}

	{extender "inventory"
		{Box
		}
	}

	{mass 1560}
	{Chassis "wheel"
		("tank_trace_ex" fx("tracks_sm2") step(0.1) len(0.6) boneleft("FXtraceL3") boneright("FXtraceR3"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("mobility_car"
			speed(70) ;----------------------------------Max speed.
			speedatmaxturn(10) ;-------------------------Max speed at max turn.
			reverse(14) ;--------------------------------Max speed in Reverse.
			weight(1.5) ;--------------------------------Weight in Tonnes.
			power(36) ;----------------------------------Horse Power.
			fuel(42) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(190) ;---------------------------------Range in Kilometers.
		)
	}
}